{
  "name": "credstash-promise",
  "version": "2.1.2",
  "description": "credstash wrapper that exposes promises and typescript definitions",
  "main": "src/index.js",
  "typings": "src/index.d.ts",
  "files": [
    "src/*.d.ts",
    "src/*.js"
  ],
  "scripts": {
    "2npm": "tsc --declaration && publish",
    "test": "MOCHA_FILE=./output/junit.xml nyc mocha --reporter mocha-multi --reporter-options spec=-,mocha-junit-reporter=-",
    "test:mocha": "mocha",
    "test:watch": "mocha --watch"
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@stridespark/credstash": "^1.0.2"
  },
  "repository": "StrideSpark/credstash-promise",
  "devDependencies": {
    "@types/chai": "^3.4.35",
    "@types/mocha": "^2.2.40",
    "@types/node": "6.0.31",
    "aws-sdk": "^2.32.0",
    "chai": "git://github.com/stridespark/chai.git#fix-572",
    "mocha": "^3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "mocha-multi": "^0.10.0",
    "mocha-notifier-reporter": "^0.1.2",
    "nyc": "^10.1.2",
    "publish": "0.6.0",
    "source-map-support": "^0.4.14",
    "typescript": "^2.2.1"
  },
  "nyc": {
    "reporter": [
      "text-summary",
      "html"
    ],
    "sourceMap": true,
    "instrument": true
  }
}
